{{#prettify}}
<!DOCTYPE html>
<html lang="en">
  <head>
    {{!-- Meta, title, CSS, favicons, etc. --}}
    {{> head }}
    {{!-- Place anything custom after this --}}
  </head>
  <body>
    <a class="sr-only" href="#content">Skip navigation</a>

    <!-- Navbar -->
    {{> nav-main }}

    <!-- Content -->
    <div class="container">
      <h1>{{title}}</h1>
      {{pager pagination ''}}
      {{> body }}
      {{> disqus }}
    </div>


    <!-- JS and analytics only. -->
    {{> footer }}

  </body>
</html>
{{/prettify}}
